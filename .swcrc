{
    "$schema": "https://json.schemastore.org/swcrc",
    "jsc": {
      "parser": {
        "syntax": "typescript",
        "tsx": false,
        "decorators": true,
        "dynamicImport": true
      },
      "transform": {
        "legacyDecorator": true,
        "decoratorMetadata": true
      },
      "target": "es2021",
      "loose": false,
      "externalHelpers": false
      // Requires v1.2.50 or upper and requires target to be es2016 or upper.
    },
    "module": {
      "type": "commonjs",
  
      // These are defaults.
      "strict": true,
      "strictMode": true,
      "lazy": false,
      "noInterop": false
    },
    "minify": false,
    "sourceMaps": true
  }